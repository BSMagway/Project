<UserControl x:Class="Project.Views.UserControls.LoadUserControl.LoadUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Project.Views.UserControls.LoadUserControl"
             xmlns:vml="clr-namespace:Project.ViewModels"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <!--<UserControl.DataContext>
        <vml:LoadUserViewModel/>
    </UserControl.DataContext>-->
    <Grid>
        <ListBox Grid.Column="0" ItemsSource="{Binding LoadedListTests}">
            <ListBox.ItemTemplate>
                <DataTemplate>
                    <StackPanel Margin="5">
                        <TextBlock FontSize="18" Text="{Binding Path=TestNumber}" />
                        <TextBlock Text="{Binding Path=TestDate}" />
                        <TextBlock Text="{Binding Path=TestId}" />
                    </StackPanel>
                </DataTemplate>
            </ListBox.ItemTemplate>
        </ListBox>

    </Grid>
</UserControl>
